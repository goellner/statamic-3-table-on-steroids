{
    "name": "goellner/table-on-steroids",
    "description": "Adds a CSV parser for the table fieldtype",
    "homepage": "https://github.com/goellner/statamic-3-table-on-steroids",
    "autoload": {
        "psr-4": {
            "Goellner\\TableOnSteroids\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goellner\\TableOnSteroids\\Tests\\": "tests"
        }
    },
    "require": {
        "statamic/cms": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.8"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Table On Steroids",
            "description": "Adds a CSV parser for the table fieldtype"
        },
        "laravel": {
            "providers": [
                "Goellner\\TableOnSteroids\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
